{
    "problemMatcher": [
        {
            "owner": "{tool-name}-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):(\\d+):\\s+(\\w+\\d+\\s+.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        }
    ]
}
